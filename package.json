    "valid-url": "^1.0.9",
    "wordpress": "git@github.com:NathanKleekamp/node-wordpress.git#rejectUnauthorized_ssl_setting"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "eslint": "^2.10.2",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.2.2",
    "eslint-plugin-react": "^5.1.1",
    "knex-csv-seeder": "^0.3.4",
    "mocha": "^2.4.5",
