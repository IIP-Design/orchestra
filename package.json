    "valid-url": "^1.0.9",
    "wordpress": "^1.3.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "knex-csv-seeder": "^0.4.1",
    "mocha": "^3.0.0",
    "nodemon": "^1.9.1",
    "underscore": "^1.8.3"
